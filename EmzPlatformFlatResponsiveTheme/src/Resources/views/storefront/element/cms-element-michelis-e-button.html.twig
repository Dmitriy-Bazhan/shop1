{% block cms_element_michelis_e_button %}
    {% set translatedConfig = element.translated.config %}
    {% set url = translatedConfig.url.value ?? 'javascript:void(0)' %}

    <div class="michelis-e-button">
        <a href="{{ url }}" {% if translatedConfig.newTab.value %}target="_blank"{% endif %}>{{ translatedConfig.btnLabel.value }}</a>
    </div>
{% endblock %}