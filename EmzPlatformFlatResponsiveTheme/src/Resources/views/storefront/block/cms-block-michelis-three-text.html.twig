{% block block_three_text %}
    {% set id = element.id %}

    <div class="m-block-three-text-up">
        {% block block_three_text_up %}
            {% set element = block.slots.getSlot('upText') %}

            <div class="col-md-12" data-cms-element-id="{{ element.id }}">
                {% block block_three_text_up_inner %}
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                {% endblock %}
            </div>
        {% endblock %}
    </div>


    <div class="m-block-three-text-under">
        {% block block_three_text_left %}
            {% set element = block.slots.getSlot('leftText') %}

            <div class="m-block-three-text-left" data-cms-element-id="{{ element.id }}">
                {% block block_three_text_left_inner %}
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                {% endblock %}
            </div>
        {% endblock %}

        {% block block_three_text_right %}
            {% set element = block.slots.getSlot('rightText') %}

            <div class="m-block-three-text-right" data-cms-element-id="{{ element.id }}">
                {% block block_three_text_right_inner %}
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                {% endblock %}
            </div>
        {% endblock %}
    </div>

{% endblock %}